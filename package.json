    "prisma:seed": "ts-node prisma/seed.ts",
    "lint": "eslint src --ext .ts",
    "test": "echo \"No tests specified yet\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "sos",
    "emergency",
    "police",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
